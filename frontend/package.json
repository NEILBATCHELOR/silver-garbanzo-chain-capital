  "scripts": {
    "dev": "vite",
    "server:build": "tsc -p tsconfig.server.json",
    "server:dev": "tsx watch server.ts",
    "server": "node --experimental-specifier-resolution=node dist-server/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "build:all": "npm run build && npm run server:build",
    "build": "vite build --mode production",
    "build:optimized": "vite build --mode production --config vite.config.optimization.ts",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "build-with-types": "tsc ; vite build",
    "build-no-errors": "tsc ; vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:circular": "madge --circular --extensions ts,tsx src/",
    "preview": "vite preview",
    "preview:optimized": "vite preview --config vite.config.optimization.ts",
    "size:check": "bundlesize check",
    "clean": "rm -rf node_modules dist .turbo .vite",
    "postinstall": "patch-package",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "types:validate": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/validate-types.ts",
    "types:update-db": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/update-database-types.ts",
    "types:generate-mapper": "node scripts/generate-type-mapper.js",
    "types:generate-guard": "node scripts/generate-type-guard.js",
    "types:generate-missing": "node scripts/generate-missing-types.js",
    "types:suggest-models": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/suggest-central-models.ts",
    "types:fix-inconsistencies": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/fix-type-inconsistencies.ts",
    "test": "vitest",
    "activity:monitor": "tsx scripts/activity/monitor.ts",
    "activity:health": "tsx scripts/activity/health-check.ts",
    "activity:metrics": "tsx scripts/activity/metrics.ts",
    "activity:status": "tsx scripts/activity/status.ts",
    "activity:performance": "vitest run src/services/activity-v2/__tests__/",
    "activity:migration-status": "tsx scripts/activity/migration-status.ts",
    "contracts:copy-artifacts": "node scripts/copy-contract-artifacts.js",
    "contracts:deploy-factory": "node scripts/deploy-token-factory.js",
    "contracts:deploy-factory-mumbai": "node scripts/deploy-token-factory.js polygonMumbai",
    "contracts:deploy-factory-sepolia": "node scripts/deploy-token-factory.js sepoliaEth",
    "test:erc721-enhanced": "./scripts/test-enhanced-erc721-integration.sh",
    "compile:erc721-enhanced": "./scripts/compile-enhanced-erc721.sh",
    "test:erc1155-enhanced": "./scripts/test-enhanced-erc1155-integration.sh",
    "compile:erc1155-enhanced": "./scripts/compile-enhanced-erc1155.sh"
