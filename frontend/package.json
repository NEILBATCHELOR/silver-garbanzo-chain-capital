  "type": "module",
  "sideEffects": [
    "src/infrastructure/guardian/initCrypto.ts",
    "src/globalPolyfills.ts",
    "src/cryptoPolyfills.ts",
    "src/infrastructure/web3/solanaPreload.ts",
